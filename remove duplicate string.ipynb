    "names = []\n",
    "x = int(input(\"how many names you want to enter \"))\n",
    "for i in range(x):\n",
    "    print(i+1,\"enter your name\")\n",
    "    names.append(input())\n",
    "s = set(names)\n",
    "l = list(s)\n",
    "for j in l :\n",
    "    print(j)"
